# AWS Configuration
# These values will be automatically filled after running the CDK deployment

# Cognito Identity Pool ID (from CDK output)
VITE_AWS_IDENTITY_POOL_ID=us-east-1:xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx

# AWS Region
VITE_AWS_REGION=us-east-1

# S3 Photos Bucket Name (from CDK output)
VITE_S3_PHOTOS_BUCKET=tv-slideshow-photos-xxxxxxxxxxxx

# Optional: CloudFront Distribution Domain
VITE_CLOUDFRONT_DOMAIN=xxxxxxxxxxxxxx.cloudfront.net

# Development Settings
# Set to true for local development with mock data
VITE_USE_MOCK_DATA=false

# Optional: Custom S3 endpoint for local testing (e.g., LocalStack)
# VITE_S3_ENDPOINT=http://localhost:4566
